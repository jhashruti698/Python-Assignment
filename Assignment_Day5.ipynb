      "source": [
        "# Use filter function to filter out the elements from a list that are divisible by 3 & 5.\n",
        "\n",
        "def fun(n):\n",
        "  return n % 3 == 0 and n % 5 == 0\n",
        "\n",
        "li = [10, 15, 35, 27, 30]\n",
        "\n",
        "f = list(filter(fun, li))\n",
        "print(\"List elements are :\", f)"
      ]
    }
